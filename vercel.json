{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "api/video-config-get.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/replace-video.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/upload-file.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/get-upload-url.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/video-config",
      "dest": "/api/video-config-get.js"
    },
    {
      "src": "/api/replace-video",
      "dest": "/api/replace-video.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/upload-file",
      "dest": "/api/upload-file.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/api/get-upload-url",
      "dest": "/api/get-upload-url.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/admin.html",
      "dest": "/admin.html"
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/(sitemap\\.xml|robots\\.txt)",
      "headers": { 
        "Content-Type": "application/xml; charset=utf-8",
        "Cache-Control": "public, max-age=3600, must-revalidate"
      },
      "dest": "/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot|mp4))",
      "headers": { 
        "Content-Type": "application/javascript; charset=utf-8",
        "Cache-Control": "public, max-age=31536000, immutable" 
      },
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}